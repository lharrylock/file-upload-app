{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "downlevelIteration": true,
    "jsx": "React",
    "lib": ["DOM", "ES2015", "ES6", "DOM.Iterable", "ScriptHost"],
    "moduleResolution": "Node",
    "noEmit": true,
    "preserveConstEnums": true,
    "skipLibCheck": true,
    "strict": true,
    "target": "ES5",
    "baseUrl": ".",
    "paths": {
      "@aics/aics-react-labkey": ["src/renderer/@types/@aics/aics-react-labkey"]
    }
  },
  "include": [
    "src/**/*"
  ]
}
